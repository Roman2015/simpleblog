<h1>Sessions#new</h1>
<p>Find me in app/views/sessions/new.html.erb</p>

<%= form_tag '/login' do %>
<div>
	<%= label_tag :email %>
	<%= text_field_tag :email, nil, class: 'form-control' %>
</div>
<div>
	<%= label_tag :password %>
	<%= password_field_tag :password, nil, class: 'form-control' %>
</div>
<div>
	
	<%= submit_tag 'Log in',  class: 'btn btn-success' %>
</div>
<% end %>